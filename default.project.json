{
	"name": "PORT-test",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"PORTMainModule": {
				"$path": "out/backend"
			}
		},
		"ReplicatedStorage":{
			"include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			}
		}
	}
}
